# FloGuru Environment Variables
# Copy this to .env and fill in your actual values

# ============================================================
# AI PROVIDERS
# ============================================================

# DeepSeek - For Reasoner Guru
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Google Gemini - For Coder & Browser Gurus
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI - Backup
OPENAI_API_KEY=your_openai_api_key_here

# ============================================================
# CHAT PLATFORMS
# ============================================================

# Telegram - Get from @BotFather
TELEGRAM_TOKEN=your_telegram_bot_token_here

# Discord - Get from Discord Developer Portal
DISCORD_TOKEN=your_discord_bot_token_here

# WhatsApp - Get from Twilio or Meta Developer
WHATSAPP_TOKEN=your_whatsapp_token_here

# ============================================================
# SERVER
# ============================================================

PORT=8420
NODE_ENV=development

# ============================================================
# DATABASE (Optional)
# ============================================================

DATABASE_URL=postgresql://user:password@localhost:5432/floguru
